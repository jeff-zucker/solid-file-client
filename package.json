{
  "scripts": {
    "test": "jest --coverage",
    "build": "babel src --out-dir js",
    "prepublish": "npm run build && webpack --optimize-minimize",
    "watch": "babel --watch src --out-dir js"
  },
  "name": "solid-file-client",
  "version": "1.0.0",
  "description": "A library for creating and managing files and folders in Solid data stores",
  "main": "dist/index.js",
  "keywords": [
    "Solid",
    "Linked Data"
  ],
  "author": "Jeff Zucker <dubzed@gmail.com>",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-flow-strip-types",
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "rdflib": "^0.19.1",
    "solid-auth-client": "^2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.87.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
