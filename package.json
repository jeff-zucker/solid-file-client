{
  "name": "solid-file-client",
  "version": "1.0.0",
  "description": "A library for creating and managing files and folders in Solid data stores",
  "author": "Jeff Zucker",
  "license": "MIT",
  "repository": "https://github.com/jeff-zucker/solid-file-client/",
  "keywords": [
    "Solid",
    "Linked Data"
  ],
  "scripts": {
    "test": "npm run test:ls -- --coverage",
    "test:ls": "TEST_PREFIX=app://ls/ jest",
    "test:file": "TEST_PREFIX=file:// jest",
    "test:https": "TEST_PREFIX=https:// jest",
    "test:watch": "npm run test:ls -- --watch",
    "test:all": "npm run test:ls && npm run test:file && npm run test:https",
    "prepublishOnly": "npm run build && npm run test:all",
    "build": "webpack",
    "build:dev": "webpack --mode development --watch",
    "build:docs": "jsdoc2md src/*.js > docs/api.md",
    "lint": "standard src/* tests/*",
    "lint:fix": "npm run lint -- --fix"
  },
  "main": "dist/node/solid-file-client.bundle.js",
  "browser": "dist/window/solid-file-client.bundle.js",
  "module": "src/index.js",
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "standard": {
    "globals": [
      "jest",
      "test",
      "describe",
      "beforeAll",
      "beforeEach",
      "afterAll",
      "afterEach",
      "expect"
    ]
  },
  "peerDependencies": {
    "solid-auth-cli": "1.0.10",
    "solid-auth-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-flow-strip-types": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0-beta.6",
    "jest": "^24.9.0",
    "jsdoc-to-markdown": "^5.0.3",
    "n3": "^1.3.3",
    "regenerator-runtime": "^0.13.3",
    "solid-auth-cli": "^1.0.10",
    "solid-namespace": "^0.2.0",
    "solid-rest": "^1.0.9",
    "standard": "^14.3.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "debug": "^4.1.1"
  }
}
